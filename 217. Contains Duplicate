Edit
nums = [1, 2, 3, 4, 2]

# Iteration 1: num = 1 â†’ not in h â†’ add to h â†’ h = {1}
# Iteration 2: num = 2 â†’ not in h â†’ add to h â†’ h = {1, 2}
# Iteration 3: num = 3 â†’ not in h â†’ add to h â†’ h = {1, 2, 3}
# Iteration 4: num = 4 â†’ not in h â†’ add to h â†’ h = {1, 2, 3, 4}
# Iteration 5: num = 2 â†’ already in h â†’ return True (duplicate found)
